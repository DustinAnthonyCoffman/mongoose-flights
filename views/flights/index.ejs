<%- include('../partials/header') %>

<a href="/flights/new">Add Flight</a>
<table>
    <thead>
     <th>Airport</th>
     <th>Airline</th>
     <th>Flight Number</th>
     <th>Departure</th>

    </thead>
    <tbody>
   
    <% flights.forEach(function(s) { %>
            <tr>
                <td><%= s.airport %></td>
                <td><%= s.airline %></td>
                <td><%= s.flightNum %></td>
                <td><%= s.departs%></td>
                <td><a href="/flights/<%= s._id %>">Details</a></td>
            </tr>
     <%   }) %>
        
    </tbody>

</table>

<%- include('../partials/footer') %>